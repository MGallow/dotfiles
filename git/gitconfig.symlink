[include]
    path = ~/.gitconfig.local
[hub]
    protocol = https
[color]
    ui = true
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
    autocrlf = input
[init]
    defaultBranch = main
[pull]
    rebase = false
[push]
    default = simple
[merge]
    ff = only
[fetch]
    prune = true
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[help]
    autocorrect = 1
